{
    "name": "kbase-narrative-core",
    "description": "Core components for the KBase Narrative Interface",
    "version": "4.3.2",
    "private": true,
    "repository": "github.com/kbase/narrative",
    "devDependencies": {
        "@types/puppeteer": "5",
        "@wdio/browserstack-service": "^6.12.1",
        "@wdio/cli": "^6.10.5",
        "@wdio/local-runner": "^6.10.5",
        "@wdio/mocha-framework": "^6.11.0",
        "@wdio/selenium-standalone-service": "^6.11.0",
        "@wdio/spec-reporter": "^6.6.0",
        "bower": "^1.8.12",
        "chrome-launcher": "^0.13.4",
        "chromedriver": "^90.0.0",
        "eslint": "7.25.0",
        "eslint-config-prettier": "^7.2.0",
        "grunt": "1.4.0",
        "grunt-cli": "1.3.2",
        "grunt-contrib-copy": "1.0.0",
        "grunt-regex-replace": "0.4.0",
        "husky": "^5.1.3",
        "jasmine-ajax": "4.0.0",
        "jasmine-core": "3.6.0",
        "jasmine-jquery": "2.1.1",
        "karma": "6.1.1",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "2.0.3",
        "karma-es6-shim": "1.0.0",
        "karma-firefox-launcher": "1.3.0",
